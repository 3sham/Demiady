@model Demiady.Expens

@{
    ViewBag.Title = "ازالة مصروفات";
}

<h2>ازالة مصروفات</h2>

<h3>هل انت متاكد من ازالة هذه المصروفات ؟</h3>
<div>
   
    <hr />
    <table class="table table-bordered table-striped text-center">
        <tr>
            <th class="text-center">
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th class="text-center">
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th class="text-center">
                @Html.DisplayNameFor(model => model.Value)
            </th>
            
        </tr>

      
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => modelItem.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => modelItem.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => modelItem.Value)
                </td>
               
            </tr>
        

    </table>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="submit" value="ازالة " class="btn btn-danger btn-lg" /> |
        @Html.ActionLink("الرجوع لقائمة المصروفات", "Index")
    </div>
    }
</div>
